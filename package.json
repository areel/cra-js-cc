{
  "name": "cra-js",
  "description": " Template for CRA JS Application with Cypress",
  "version": "0.1.0",
  "private": true,
  "baseUrl": "src",
  "dependencies": {
    "@craco/craco": "^5.2.4",
    "@material-ui/core": "^4.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "craco start",
    "debug": "craco --inspect-brk start",
    "build": "craco build",
    "test": "craco test",
    "cy:o": "cypress open"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "cy": true,
      "Cypress": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^1.8.0",
    "babel-loader": "^8.0.6",
    "cypress": "^3.4.1",
    "cypress-react-unit-test": "^2.4.3",
    "istanbul-lib-coverage": "^2.0.5",
    "nyc": "^14.1.1"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx",
      ".js",
      ".jsx"
    ]
  }
}
